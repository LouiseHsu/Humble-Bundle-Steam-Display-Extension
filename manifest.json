{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "permissions": [ "tabs", "activeTab", "declarativeContent", "storage", "https://store.steampowered.com/api/" ],
  "background": {
    "scripts": [ "background.js" ],
    "persistent": false
  },
   "content_security_policy": "script-src 'self' https://store.steampowered.com/api/appdetails/?appids=582010&cc=CA&filters=price_overview; object-src 'self'",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "128": "images/get_started128.png",
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png"
  },
  "options_page": "options.html",
  "manifest_version": 2
}
